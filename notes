# создание проекта
django-admin startproject mysite

# создание приложения
django-admin startapp blog

# миграция БД
python manage.py migrate

# создание миграции БД
python manage.py makemigrations app_name
python manage.py makemigrations shop --name "add_translation_model"

# запуск сервера для разработки
python manage.py runserver
python manage.py runserver --settings mysite.settings

# создание суперпользователя
python manage.py createsuperuser

# создать файлы локализации
python manage.py makemessages

# скомпилировать файлы локализации
python manage.py compilemessages

queryset = User.objects.annotate(username_index=ExpressionWrapper(Func(F('username'), Value('(\d+)'),
 function='substring'), output_field=IntegerField()))).filter(username__iregex=r'^[A-Z]{2}\d+$').values_list('username_index', flat=True)